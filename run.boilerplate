#!/usr/bin/env bash
cd "$(dirname "$BASH_SOURCE")"
repo_root=$(pwd)

# HOW TO USE:
# $ ./run /path/to/config
#
# Then copy this file as `./run` at the project root (it is ignored by git) and
# change the following config. After it's done, just start `./run` to start
# scraping and indexing.

api_key="apiKey"
app_id="appID"

# NO NEED TO CHANGE ANYTHING AFTER THIS LINE

APPLICATION_ID=$app_id API_KEY=$api_key INDEX_PREFIX="" CONFIG=$1 python src/index.py
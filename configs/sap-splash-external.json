{
  "index_name": "sap-splash-external",
  "allowed_domains": [
    "sap.github.io"
  ],
  "start_urls": [
    "http://sap.github.io/BUILD_User_Assistance/Splash/external",
    "http://sap.github.io/BUILD_User_Assistance/Splash/external/.*"
  ],
  "stop_urls": [
    "http://sap.github.io/BUILD_User_Assistance/Splash/external/index.html",
    "http://sap.github.io/BUILD_User_Assistance/Splash/external/AdminConsoleHelp.html"
  ],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": {
      "selector": "#nav-content ul ul > li.active > span",
      "default_value": "Video"
    },
    "lvl1": "h1.title",
    "lvl2": "h2.sectiontitle, h2.title, h2.section_title",
    "lvl3": "h3",
    "text": ".conbody p, .conbody li, .taskbody p, .taskbody li, .body .p"
  },
  "custom_settings": {},
  "strategy": "default",
  "min_indexed_level": 1
}
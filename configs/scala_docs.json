{
    "index_name": "scaladocs",
    "allowed_domains": [
        "docs.scala-lang.org"
    ],
    "start_urls": [
        "http://docs.scala-lang.org/de[tags:de]",
        "http://docs.scala-lang.org/es[tags:es]",
        "http://docs.scala-lang.org/fr[tags:fr]",
        "http://docs.scala-lang.org/ja[tags:ja]",
        "http://docs.scala-lang.org/ko[tags:ko]",
        "http://docs.scala-lang.org/pt-br[tags:pt-br]",
        "http://docs.scala-lang.org/zh-cn[tags:zh-cn]",
        "http://docs.scala-lang.org/overviews[tags:en,learn]",
        "http://docs.scala-lang.org/tutorials[tags:en,learn]",
        "http://docs.scala-lang.org/style[tags:en,learn]",
        "http://docs.scala-lang.org/glossary[tags:en,quickref]",
        "http://docs.scala-lang.org/cheatsheets[tags:en,quickref]",
        "http://docs.scala-lang.org/contribute[tags:en,contribute]",
        "http://docs.scala-lang.org/sips[tags:en,sips]",
        "http://docs.scala-lang.org[tags:en,homepage]"
    ],
    "stop_urls": [
      "http://docs.scala-lang.org/index.html",
      "http://docs.scala-lang.org/overviews/index.html",
      "http://docs.scala-lang.org/overviews/macros/quasiquotes.html"
    ],
    "selectors_exclude": [
      ".contents",
      ".copyright",
      ".footer",
      "#scroller"
    ],
    "selectors": [
      ["header h1", ".wrapper h1", ".container h1",  ".container #guide-title"],
      [".container h1"],
      [".container h2"],
      [".container h3"],
      [".container h4"],
      ["p"]
    ],
    "custom_settings": {
      "attributesForFaceting": ["title"],
      "attributesToIndex": ["title", "h1", "h2", "h3", "h4", "h5", "h6", "content"],
      "attributesToSnippet": ["content:20"]
    },
    "hash_strategy": "id",
    "strategy": "laravel",
    "strip_chars": " "
}
{
  "index_name": "hexo_zh-cn",
  "allowed_domains": [
    "hexo.io"
  ],
  "start_urls": [
    "https://hexo.io/zh-cn/docs/",
    "https://hexo.io/zh-cn/api/"
  ],
  "stop_urls": [
  	"https://hexo.io/zh-cn/docs/index.html",
    "https://hexo.io/zh-cn/api/index.html"
  ],
  "selectors_exclude": [
  ],
  "selectors": {
  	"lvl0": ".article-header h1",
    "lvl1": ".article-content h1",
    "lvl2": ".article-content h2",
    "lvl3": ".article-content h3",
    "lvl4": ".article-content h4",
    "text": ".article-content p"
  },
  "custom_settings": {},
  "strategy": "default",
  "strip_chars": " :;,.",
  "min_indexed_level": 1
}
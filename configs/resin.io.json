{
  "index_name": "resin.io",
  "start_urls": [
    "http://docs.resin.io"
  ],
  "stop_urls": [
    "http://docs.resin.io/#/pages/tools"
  ],
  "selectors_exclude": [
    "#further-reading",
    "#feedback"
  ],
  "selectors": {
    "lvl0": {
      "selector": "//header//li[1]",
      "type": "xpath",
      "global": true
    },
    "lvl1": "header li.active",
    "lvl2": ".content-wrapper h2",
    "lvl3": ".content-wrapper h3",
    "lvl4": ".content-wrapper h4",
    "lvl5": ".content-wrapper h5",
    "text": ".content-wrapper p"
  },
  "custom_settings": {},
  "strategy": "default",
  "js_render": true,
  "js_wait": 2,
  "strip_chars": " :;,.#",
  "use_anchors": true
}
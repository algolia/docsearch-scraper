{
    "index_name": "easylogin-pro",
    "start_urls": [
      "http://docs.hazzardweb.com/easylogin-pro/1.2"
    ],
    "stop_urls": [
      "/./"
    ],
    "selectors_exclude": [
    ],
    "selectors": {
      "lvl0": {
        "selector": "//nav[@class=\"docs-toc\"]/ul/li/ul/li[@class=\"active\"]/../preceding::p[1]",
        "type": "xpath",
        "global": "true"
      },
      "lvl1": ".docs-content h1",
      "lvl2": ".docs-content h2",
      "lvl3": ".docs-content h3",
      "lvl4": ".docs-content h4",
      "lvl5": ".docs-content h5",
      "text": ".docs-content p"
    },
    "js_render": true,
    "js_wait": 2,
    "min_indexed_level": 1,
    "custom_settings": {},
    "strategy": "default"
}

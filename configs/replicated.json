{
  "index_name": "replicated",
  "start_urls": [
    "http://docs.replicated.com/docs"
  ],
  "stop_urls": [
    "/edit$"
  ],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": {
        "selector": "//*[@class=\"sidebar-nav\"]/ul/li/*[@class=\"active\"]/../../preceding::h4[1]",
        "type": "xpath",
        "global": true,
        "default": "Documentation"
    },
    "lvl1": ".docs-header h1",
    "lvl2": ".content h1",
    "lvl3": ".content h2",
    "lvl4": ".content h3",
    "text": ".content p, .content li"
  },
  "min_indexed_level": 1,
  "custom_settings": {},
  "strategy": "default",
  "js_render": true,
  "download_timeout": 30
}

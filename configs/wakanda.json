{
  "index_name": "wakanda",
  "start_urls": [
    "http://wakanda.github.io/angular-wakanda/#/doc/developer-guide/",
    "http://wakanda.github.io/angular-wakanda/#/doc/api-reference/"
  ],
  "stop_urls": [
  ],
  "selectors_exclude": [
  ],
  "selectors": {
    "lvl0": {
      "selector": "//ul[contains(@class, \"nav-sidebar\")]/li[contains(@class, \"active\")]/../preceding::a[1]",
      "type": "xpath",
      "global": true
    },
    "lvl1": ".main h1",
    "lvl2": ".main h2",
    "lvl3": ".main h3",
    "lvl4": ".main h4",
    "lvl5": ".main h5",
    "text": ".main p"
  },
  "custom_settings": {},
  "js_render": true,
  "js_wait": 2,
  "min_indexed_level": 1,
  "use_anchors": true
}

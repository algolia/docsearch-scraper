{
  "index_name": "lingohub",
  "allowed_domains": [
    "lingohub.com"
  ],
  "start_urls": [
    "https://lingohub.com/documentation/guides",
    "https://lingohub.com/documentation/developers"
  ],
  "stop_urls": [
    ".*_wpnonce.*"
  ],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": {
      "selector": ".sidebar-nav > li.current_page_item > a, .sidebar-nav .current_page_ancestor > a",
      "global": true
    },
    "lvl1": {
      "selector": ".sidebar-nav > li > ul > li a.current_page_item",
      "global": true
    },
    "lvl2": "div.entry-content.content h2",
    "lvl3": "div.entry-content.content h3",
    "text": "div.entry-content.content p, div.entry-content.content li"
  },
  "custom_settings": {},
  "strategy": "default",
  "min_indexed_level": 2
}
{
  "index_name": "marionettejs",
  "allowed_domains": [
    "marionettejs.com"
  ],
  "start_urls": [
    "http://marionettejs.com/docs/v2.4.4/"
  ],
  "stop_urls": [],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": "div.docs div.docs__content h1",
    "lvl1": "div.docs div.docs__content h2",
    "lvl2": "div.docs div.docs__content h3",
    "text": "div.docs div.docs__content p, div.docs div.docs__content li"
  },
  "custom_settings": {},
  "strategy": "default",
  "strip_chars": " :;,."
}

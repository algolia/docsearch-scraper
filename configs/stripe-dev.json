{
    "index_name": "stripe-dev",
    "allowed_domains": ["stripe.com"],
    "start_urls": [
        "https://stripe.com/docs"
    ],
    "stop_urls": [
        "https://stripe.com/docs/api",
        "https://stripe.com/docs/mobile",
        "https://stripe.com/docs/connect",
        "https://stripe.com/docs/guides",
        "https://stripe.com/docs/tutorials",
        "https://stripe.com/docs/relay",
        "https://stripe.com/docs/s",
        "https://stripe.com/docs/f",
        "https://stripe.com/docs/e",
        "https://stripe.com/docs/w",
        "https://stripe.com/docs/r",
        "https://stripe.com/docs/l",
        "https://stripe.com/docs/t",
        "https://stripe.com/docs/u",
        "https://stripe.com/docs/i"
    ],
    "selectors_exclude": [
        ".method-list"
    ],
    "selectors": {
      "lvl0": "#content header h1,#content section h1",
      "lvl1": "#content section h2",
      "lvl2": "#content section h3",
      "lvl3": "#content section h4",
      "lvl4": "#content section h5",
      "lvl5": "#content section h6",
      "text": "#content header p,#content section p,#content section ol"
    },
    "custom_settings": {},
    "hash_strategy": "id",
    "strategy": "default",
    "strip_chars": " :;,."
}


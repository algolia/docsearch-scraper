{
  "index_name": "docsearch",
  "allowed_domains": [
    "community.algolia.com"
  ],
  "start_urls": [
    "https://community.algolia.com/docsearch/documentation/"
  ],
  "stop_urls": [],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": ".main-content h2",
    "lvl1": ".main-content h3",
    "lvl2": ".main-content h4",
    "lvl3": ".main-content h5",
    "text": ".main-content p, .main-content ol"
  },
  "min_indexed_level": 1,
  "custom_settings": {},
  "strategy": "default",
  "strip_chars": " :;,."
}
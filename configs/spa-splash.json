{
  "index_name": "sap-splash",
  "allowed_domains": [
    "sap.github.io"
  ],
  "start_urls": [
    "http://sap.github.io/BUILD_User_Assistance/Splash/",
    "http://sap.github.io/BUILD_User_Assistance/Splash/.*"
  ],
  "stop_urls": [
    "http://sap.github.io/BUILD_User_Assistance/Splash/index.html",
    "http://sap.github.io/BUILD_User_Assistance/Splash/video.html",
    "http://sap.github.io/BUILD_User_Assistance/Splash/AdminConsoleHelp.html"
  ],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": "#nav-content ul ul > li.active > span",
    "lvl1": "h1.title",
    "lvl2": "h2.sectiontitle, h2.title, h2.section_title",
    "lvl3": "h3",
    "text": ".conbody p, .conbody li, .taskbody p, .taskbody li, .body .p"
  },
  "custom_settings": {},
  "strategy": "default",
  "min_indexed_level": 1
}
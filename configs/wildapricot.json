{
  "index_name": "wildapricot",
  "start_urls": [
    "http://help.wildapricot.com/display/DOC"
  ],
  "stop_urls": [
    "from=.*$",
    "jsessionid=.*$",
    "http://help.wildapricot.com/display/DOC/Browser\\+application\\+release\\+history",
    "Update+.*$",
    "Release+.*$"
  ],
  "selectors_exclude": [
  ],
  "selectors": {
    "lvl0": ".wiki-content h1",
    "lvl1": ".wiki-content h2",
    "lvl2": ".wiki-content h3",
    "lvl3": ".wiki-content h4",
    "text": ".wiki-content p"
  },
  "js_render": true,
  "min_indexed_level": 2
}
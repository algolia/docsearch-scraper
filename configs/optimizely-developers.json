{
  "index_name": "optimizely-developers",
  "allowed_domains": [
    "developers.optimizely.com"
  ],
  "start_urls": [
    "http://developers.optimizely.com/"
  ],
  "stop_urls": [
    "http://developers.optimizely.com/android/help/reference/.*",
    "http://developers.optimizely.com/ios/help/html/.*",
    "index.html"
  ],
  "selectors_exclude": [],
  "selectors": {
    "lvl0": "#nav > .nav-sidebar > ul > li > a.active, #content h1",
    "lvl1": "#content h2",
    "lvl2": "#content h3",
    "lvl3": "#content h4",
    "lvl4": "#content h5",
    "text": "#content p, #content li"
  },
  "strategy": "default",
  "min_indexed_level": 2
}

{
    "index_name": "chef",
    "allowed_domains": ["docs.chef.io"],
    "start_urls": [
        "https://docs.chef.io/",
        "https://docs.chef.io/resource[page_rank:1]"
    ],
    "stop_urls": [
      "https://docs.chef.io/release"
    ],
    "selectors_exclude": [
        ".headerlink"
    ],
    "selectors": [
        [".body h1"],
        [".body h2"],
        [".body h3"],
        [".body .docutils table tr td:nth-of-type(1)"],
        [".body p", ".body .docutils table tr td:nth-of-type(2)"]
    ],
    "custom_settings": {
        "attributesToIndex": ["text_title", "unordered(text_h1)", "unordered(text_h2)", "title",
                                "unordered(h1)", "unordered(h2)", "content", "path", "hash"]
    },
    "hash_strategy": "id",
    "strategy": "laravel"
}
{
  "index_name": "getpostman",
  "allowed_domains": [
    "www.getpostman.com"
  ],
  "start_urls": [
    "https://www.getpostman.com/docs/"
  ],
  "stop_urls": [],
  "selectors_exclude": [
  	"#header"
  ],
  "selectors": {
    "lvl0": ".content h2",
    "lvl1": ".content h3",
    "lvl2": ".content h4",
    "lvl3": ".content h5",
    "text": ".content p"
  },
  "custom_settings": {},
  "strategy": "default",
  "strip_chars": " :;,.",
  "min_indexed_level": 3
}

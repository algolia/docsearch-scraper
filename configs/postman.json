{
  "index_name": "getpostman",
  "allowed_domains": [
    "www.getpostman.com"
  ],
  "start_urls": [
    "https://www.getpostman.com/docs/"
  ],
  "stop_urls": [],
  "selectors_exclude": [
  	"#header"
  ],
  "selectors": {
    "lvl0": {
      "selector": "//*[@id='accordion']/*/*/li/a[@class='active']/../../../*[@class='category-title ']",
      "type": "xpath",
      "global": true
    },
    "lvl1": ".content h2",
    "lvl2": ".content h3",
    "lvl3": ".content h4",
    "lvl4": ".content h5",
    "lvl5": ".content p b",
    "text": ".content p"
  },
  "custom_settings": {},
  "strategy": "default",
  "strip_chars": " :;,."
}